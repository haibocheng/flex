<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/halo">
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
    <s:Group id="track" top="0" left="0" right="0" bottom="0">
        <s:Ellipse top="0" left="0" right="0" bottom="0" minWidth="50" minHeight="50">
            <s:fill>
                <mx:RadialGradient id="trackFill">
                    <mx:GradientEntry id="tfg1" ratio="0" color="0x808080"/>
                    <mx:GradientEntry id="tfg2" ratio=".8" color="0xFF8080"/>
                    <mx:GradientEntry id="tfg3" ratio="1" color="0xFF2020" color.disabled="0xFFD0D0"/>
                </mx:RadialGradient>
            </s:fill>
        </s:Ellipse>
        <s:Rect left="25" top="22" width="25" height="6">
            <s:fill>
                <mx:LinearGradient>
                    <mx:GradientEntry id="tpg1" color="0x808080"/>
                    <mx:GradientEntry id="tpg2" ratio="0.8" color="0x8080FF"/>
                    <mx:GradientEntry id="tpg3" color="0x0000FF" color.disabled="0x8080FF"/>
                </mx:LinearGradient>
            </s:fill>
        </s:Rect>
    </s:Group>
</s:Skin>
