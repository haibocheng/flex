<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/halo">
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
    <s:Ellipse id="track" top="0" bottom="0" width="16" minHeight="16">
        <s:fill>
            <mx:LinearGradient>
                <mx:GradientEntry id="g1" color="0xEEEEEE" color.disabled="0xEEEEEE" />
                <mx:GradientEntry id="g2" color="0xBBBBBB" color.disabled="0xDDDDDD" />
                <mx:GradientEntry id="g3" color="0xEEEEEE" color.disabled="0xEEEEEE" />
            </mx:LinearGradient>
        </s:fill>
    </s:Ellipse>
</s:Skin>
