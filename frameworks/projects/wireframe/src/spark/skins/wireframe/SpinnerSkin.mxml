<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
	  alpha.disabled="0.5" minHeight="23" minWidth="12">

    <fx:Metadata>
    	[HostComponent("spark.components.Spinner")]
    </fx:Metadata> 
    
    <s:states>
    	<s:State name="normal" />
    	<s:State name="disabled" />
    </s:states>
    
    <s:Button id="incrementButton" left="0" right="0" top="0" height="50%"  
            skinClass="spark.skins.wireframe.SpinnerIncrementButtonSkin" />
    <s:Button id="decrementButton" left="0" right="0" bottom="0" height="50%" 
            skinClass="spark.skins.wireframe.SpinnerDecrementButtonSkin" />

</s:Skin>
